.logo.small
  font-family 'Asap', sans-serif
  text-align left
  padding-top 15px
  #logo
    height 31px
    width 60px
    margin-top 3px
    margin-right 10px
  .title
    color #000
    font-size 18px
    font-weight bold
    margin-top 0px
    .tdl
      color rgba(#000,0.25)
      margin-left 3px
      font-style italic
      font-size 14px
  .slogan
    color rgba(#000, 0.25)
    font-size 11px
    // margin-top -4px
    width 100px
    line-height 12px
    // margin-left -15px
.logo.big
  font-family 'Asap', sans-serif
  text-align left
  margin-top 15px
  #logo
    height 41px
    width 90px
  .title
    color #000
    font-size 23px
    font-weight bold
    margin-top 7px
    .tdl
      color rgba(#000,0.25)
      margin-left 3px
      font-style italic
  .slogan
    color rgba(#000, 0.25)
    margin-top 2px
#pageHeader
  text-align right
  width 100%
  max-width 1000px
  nav
    .name
      font-weight normal
    a
      color rgba(#000, 0.25)
      font-weight 700
      font-size 13px
      display inline-block
      margin 45px 0 15px 15px
