#customerTeam
  .team
    width 100%
    thead th
      background #3f3f3f
      color #fff
      font-weight 600
    td, th
      padding 10px
    .member
      .picture
        width 20px
        img
          width 20px
          height 20px
          border-radius 50%
    .actions
      width 70px
      text-align right
      a
        margin-left 10px
        margin-right -5px
        &:before
          font-size 20px
